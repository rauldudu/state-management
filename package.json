  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  },
  "scripts": {
    "start": "npm-run-all server client",
    "server": "kill-port $(node env.js --value SERVER_PORT) && json-server db.json --port $(node env.js --value SERVER_PORT) --watch &",
