      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "fetch-mock": "^9.10.7",
    "json-server": "^0.16.1",
    "kill-port": "^1.6.1",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.1",
    "react-scripts": "3.4.3"
  },
  "engines": {
    "node": ">=12.0.0"
  }
